{
  "name": "jupyterlab-form",
  "version": "0.1.0",
  "description": "Interactive forms.",
  "author": "Simon Biggs",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "tsc && node-sass ./src/theme.scss ./lib/theme.css && cp ./src/style.css ./lib/style.css && cp ./src/mdi.svg ./lib/mdi.svg",
    "clean": "rimraf lib",
    "prepublish": "npm run build"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,css,jpg,js,js.map,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "extension": true
  },
  "dependencies": {
    "@angular/animations": "4.4.5",
    "@angular/cdk": "2.0.0-beta.12",
    "@angular/common": "4.4.5",
    "@angular/compiler": "4.4.5",
    "@angular/core": "4.4.5",
    "@angular/forms": "4.4.5",
    "@angular/http": "4.4.5",
    "@angular/material": "2.0.0-beta.12",
    "@angular/platform-browser": "4.4.5",
    "@angular/platform-browser-dynamic": "4.4.5",
    "@jupyterlab/application": "0.11.1",
    "@jupyterlab/outputarea": "0.11.1",
    "@jupyterlab/rendermime": "0.11.1",
    "@jupyterlab/services": "0.50.2",
    "@types/codemirror": "0.0.47",
    "@types/markdown-it": "0.0.4",
    "core-js": "^2.5.1",
    "markdown-it": "^8.4.0",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "codelyzer": "^3.2.1",
    "node-sass": "^4.5.3",
    "rimraf": "^2.6.1",
    "tslint": "^5.7.0",
    "typescript": "~2.4.0"
  }
}

{
  "name": "jupyterlab-form",
  "version": "0.1.0",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SimonBiggs/jupyterlab-form.git"
  },
  "description": "Interactive forms.",
  "author": "Simon Biggs",
  "main": "lib/jupyterlab-extension/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "npm run clean && npm run copy && tsc && npm run sass",
    "copy": "cp -r ./src ./lib ",
    "sass": "node-sass ./src/angular-component/theme.scss ./lib/angular-component/theme.css",
    "watch": "tsc -w",
    "clean": "rimraf lib",
    "prepack": "npm run build"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,css,jpg,js,js.map,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "extension": true
  },
  "dependencies": {
    "@angular/animations": "4.4.6",
    "@angular/cdk": "2.0.0-beta.8",
    "@angular/common": "4.4.6",
    "@angular/compiler": "4.4.6",
    "@angular/core": "4.4.6",
    "@angular/forms": "4.4.6",
    "@angular/http": "4.4.6",
    "@angular/material": "2.0.0-beta.8",
    "@angular/platform-browser": "4.4.6",
    "@angular/platform-browser-dynamic": "4.4.6",
    "@jupyterlab/application": "0.12.0",
    "@jupyterlab/docmanager": "0.12.0",
    "@jupyterlab/fileeditor-extension": "0.12.0",
    "@jupyterlab/launcher": "0.12.0",
    "@jupyterlab/outputarea": "0.12.0",
    "@jupyterlab/rendermime": "0.12.0",
    "@jupyterlab/services": "0.51.0",
    "@phosphor/widgets": "^1.5.0",
    "@types/codemirror": "0.0.47",
    "@types/markdown-it": "0.0.4",
    "core-js": "^2.5.1",
    "markdown-it": "^8.4.0",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "codelyzer": "^3.2.1",
    "node-sass": "^4.5.3",
    "rimraf": "^2.6.1",
    "tslint": "^5.7.0",
    "typescript": "2.4.2"
  }
}
